server {
    listen 80 default_server;
    listen [::]:80 default_server;
    root /var/www/html;
    index index.html index.htm;
    server_name {{ server_name }};
    location / {
             try_files $uri $uri/ /index.html;
    }
    location  /.well-known/acme-challenge {
               root /var/www/letsencrypt;
     }
}
